/* pages/view/view.wxss */
page{
  background: #ededed;
}
.view{
  width: 100%;
  height: 1400rpx;
  background: #ffffff;
  margin-top: 16rpx;
}
.inputList{
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: s-between;
  border-bottom: 2rpx solid #ededed;
}
label{
  font-size: 32rpx;
  height: 90rpx;
  line-height: 90rpx;
  width: 130rpx;
  margin-left: 22rpx;
  font-weight: bold
}
#name{
  width: 100%;
  height: 90rpx;
  margin-right: 24rpx;
  text-align: right;
}
.name{
  font-size: 26rpx;
  line-height: 26rpx;
  color: #999999;
}
#tel{
  width: 70%;
  height: 90rpx;
  margin-right: 24rpx;
  text-align: right;
}
.tel{
  font-size: 26rpx;
  line-height: 26rpx;

  color: #999999;
}
.textList{
  width: 100%;
  height: 400rpx;
  display: flex;
  justify-content: space-between
}
.textview{
  width: 90%;
  height: 320rpx;
  margin-right: 24rpx;
  text-align: right;
  font-size: 26rpx;
  color: #999999;
  line-height: 39rpx;
  padding: 20rpx 0;
}
/* .textviews{
  width: 90%;
  height: 320rpx;
  display: block;
  font-size: 24rpx;
  text-align: left;
  color: #999999;
} */
.upload{
  width: 278rpx;
  height: 190rpx;
  border: 2rpx solid #e6e6e6;
  margin: 20rpx 0 0 24rpx;
}
.upload_pic{
  width: 68rpx;
  height: 60rpx;
  display: block;
  margin: 32rpx auto 0;
}
.describe{
  width: 104rpx;
  height: 26rpx;
  font-size: 26rpx;
  line-height: 26rpx;
  margin: 32rpx auto 0;
}
.submit{
  margin: auto;
  margin-top: 300rpx;
  bottom: 0rpx;
  left: 0;
  width: 100%;
  height: 90rpx;
  background: #82ccd6ee;
  font-size: 30rpx;
  line-height: 98rpx;
  color: #ffffff;
  border-radius:28rpx;
}
