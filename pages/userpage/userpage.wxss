
page{
  background: #ededed;
}
.mine_info{
  width: 100%;
  height: 388rpx;
  margin-top:10rpx;
  position: relative;
}
.mine_info_head{
  width: 94%;
  height: 218rpx;
  margin: auto;
  background: #ffffff;
  position: absolute;
  top:150rpx;
  left:0;
  right: 0;
  border-radius: 28rpx;
  display: flex;
  justify-content: space-around;
}
.mine_head_left{
  width:50%;
  height: 60rpx;
  margin-top: 30rpx;
}
.mine_head_right{
  width: 50%;
  height: 78rpx;
  margin-top: 72rpx;
}
.mine_head_leftPic{
  width: 60rpx;
  height: 60rpx;
  margin-left: 5rpx;
}
.mine_head_rightPic{
  width: 50rpx;
  height: 50rpx;
  display: block;
  margin:auto;
}
.mine_head_leftDes{
  text-align: center;
  font-size: 26rpx;
  line-height: 26rpx;
  margin-top: -40rpx;
  margin-left: 40rpx;
  color:#333333;
}

.mine_head_leftContent{
  font-size: 40rpx;
  line-height: 26rpx;
  margin-top: 45rpx;
  margin-left: 120rpx;
  color:#82ccd6ee;
}

.mine_head_rightDes{
  text-align: center;
  font-size: 26rpx;
  line-height: 26rpx;
  margin-top: 16rpx;
  color:#333333;
}
.mine_head_center{
  width: 34%;
  height: 100%;
  position: relative;
}
.jiange{
  width: 2rpx;
  height: 30rpx;
  background: #e6e6e6;
  box-sizing: border-box;
  margin-top: 100rpx;
}
.headBorder{
  position: absolute;
  width: 166rpx;
  height: 166rpx;
  background: #ffffff;
  border-radius: 50%;
  top:-79rpx;
  left:0;
  right: 0;
  margin: auto;
}
.Head{
  width: 158rpx;
  height: 158rpx;
  display: block;
  clip-path: circle(79rpx at center); 
  margin: 4rpx auto;
}
.dian{
  width: 100rpx;
  height: 98rpx;
}
.nameBox{
  width: 100%;
  height: 34rpx;
  position: relative;
}
.name{
  text-align: center;
  font-size: 34rpx;
  font-weight: bold;
  margin:auto;
  position: absolute;
  left:0;
  right: 0;
  top:0;
}

.list_content{
  width: 94%;
  height: 738rpx;
  background: #ffffff;
  margin:16rpx auto;
  border-radius: 28rpx;
}
.list{
  width: 92%;
  margin:auto;
  height: 96rpx;
  border-bottom: 2rpx solid #e6e6e6;
  display: flex;
  justify-content: space-between;
}
.list_left{
  width: 600rpx;
  height: 34rpx;
  display: flex;
  margin: 36rpx 0 0 10rpx;
}
.listIcon{
  width: 50rpx;
  height: 50rpx;
}
.listName{
  width: 600rpx;
  height: 100rpx;
  display: inline-block;
  font-size: 26rpx;
  color: #404040;
  margin-top: 5rpx;
  margin-left: 10rpx;
}
.jiantou{
  width: 16rpx;
  height: 28rpx;
  margin-top: 45rpx;
}
.list_right{
  width: 84rpx;
  height: 28rpx;
  display: flex;
  justify-content: space-between;

}
.check{
  margin-left: -120rpx;
  font-size: 24rpx;
  line-height: 28rpx;
  color:#999999;
  margin-top: 45rpx;
}
.signOut{
  width: 84%;
  height: 78rpx;
  margin: auto;
  font-size: 30rpx;
  color: #ffffff;
  border-radius: 39rpx;
  background: #82ccd6ee;
  margin-top: 240rpx;
}
